{
  "name": "learn-javascript",
  "version": "0.0.1",
  "description": "Learn JavaScript",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,mjs,html}\"",
    "test": "pnpm /^test:.*/",
    "test:unit": "jest",
    "test:examples": "node ./testing/self/examples.test.mjs",
    "watch:test:unit": "jest --watch",
    "watch": "pnpm run watch:test:unit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exiguus/learn-javascript.git"
  },
  "keywords": [
    "JavaScript",
    "OOP",
    "functional",
    "programming",
    "testing",
    "asynchronously",
    "DOM",
    "Node.js",
    "Web Components",
    "React"
  ],
  "author": "Simon Gattner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exiguus/learn-javascript/issues"
  },
  "homepage": "https://github.com/exiguus/learn-javascript#readme",
  "devDependencies": {
    "husky": "^8.0.3",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "jest": "^29.7.0"
  }
}